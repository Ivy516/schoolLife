<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/iv_class"
        android:layout_width="88dp"
        android:layout_height="73dp"
        android:src="@drawable/ic_class"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.100000024" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="我的课表"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/iv_class"
        app:layout_constraintStart_toStartOf="@+id/iv_class"
        app:layout_constraintTop_toBottomOf="@+id/iv_class"
        app:layout_constraintVertical_bias="0.00999999" />

    <ImageView
        android:id="@+id/iv_chaxun"
        android:layout_width="88dp"
        android:layout_height="73dp"
        android:src="@drawable/ic_chaxun"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toEndOf="@+id/iv_class"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:text="查询"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="@+id/iv_chaxun"
        app:layout_constraintStart_toStartOf="@+id/iv_chaxun"
        app:layout_constraintTop_toBottomOf="@+id/iv_chaxun" />

    <ImageView
        android:id="@+id/iv_xiaoli"
        android:layout_width="88dp"
        android:layout_height="73dp"
        android:src="@drawable/ic_xiaoli"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.39"
        app:layout_constraintStart_toEndOf="@+id/iv_chaxun"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.100000024" />

    <TextView
        android:id="@+id/tv_xioali"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:text="校历"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="@+id/iv_xiaoli"
        app:layout_constraintStart_toStartOf="@+id/iv_xiaoli"
        app:layout_constraintTop_toBottomOf="@+id/iv_xiaoli" />

    <ImageView
        android:id="@+id/iv_second_shop"
        android:layout_width="88dp"
        android:layout_height="73dp"
        android:src="@drawable/ic_second_shop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/iv_class"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toStartOf="@+id/iv_class"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.050000012" />

    <TextView
        android:id="@+id/tv_second_shop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="二手市场"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/iv_second_shop"
        app:layout_constraintStart_toStartOf="@+id/iv_second_shop"
        app:layout_constraintTop_toBottomOf="@+id/iv_second_shop"
        app:layout_constraintVertical_bias="0.01999998" />


    <ImageView
        android:id="@+id/iv_map"
        android:layout_width="88dp"
        android:layout_height="73dp"
        android:src="@drawable/ic_map"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/iv_chaxun"
        app:layout_constraintStart_toStartOf="@+id/iv_chaxun"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.050000012" />

    <TextView
        android:id="@+id/tv_map"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="地图"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/iv_map"
        app:layout_constraintStart_toStartOf="@+id/iv_map"
        app:layout_constraintTop_toBottomOf="@+id/iv_map"
        app:layout_constraintVertical_bias="0.01999998"/>


</android.support.constraint.ConstraintLayout>